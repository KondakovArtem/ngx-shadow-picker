{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-shadow-picker": {
      "projectType": "library",
      "root": "projects/ngx-shadow-picker",
      "sourceRoot": "projects/ngx-shadow-picker/src",
      "prefix": "shadow",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-shadow-picker/ng-package.json",
            "tsConfig": "projects/ngx-shadow-picker/tsconfig.lib.prod.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-shadow-picker/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-shadow-picker/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-shadow-picker/src/test.ts",
            "tsConfig": "projects/ngx-shadow-picker/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-shadow-picker/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "ngx-shadow-picker"
}
